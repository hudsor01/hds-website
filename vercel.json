{
  "framework": "nextjs",
  "buildCommand": "next build",
  "functions": {
    "src/app/api/*/route.ts": {
      "runtime": "edge",
      "maxDuration": 10
    },
    "src/app/api/contact/route.ts": {
      "maxDuration": 10
    },
    "src/app/blog/[slug]/page.tsx": {
      "maxDuration": 60
    },
    "src/app/sitemap.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/blog/rss",
      "destination": "/api/rss/feed"
    },
    {
      "source": "/blog/feed",
      "destination": "/api/rss/feed"
    },
    {
      "source": "/rss",
      "destination": "/api/rss/feed"
    },
    {
      "source": "/feed",
      "destination": "/api/rss/feed"
    }
  ],
  "redirects": [
    {
      "source": "/portfolio",
      "destination": "/services",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"]
}