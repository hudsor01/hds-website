{
  "name": "hudson-digital-solutions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:both": "BUNDLE_ANALYZE=both next build",
    "bundle-report": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run bundle-report",
    "size-check": "npx bundlesize",
    "security:validate": "node scripts/validate-security.cjs",
    "security:startup": "node scripts/startup-validation.cjs",
    "security:full": "npm run security:validate && npm run security:startup",
    "production:validate": "npm run type-check && npm run lint && npm run security:full",
    "production:build": "npm run production:validate && npm run build",
    "production:start": "npm run security:startup && npm start",
    "security:test": "jest __tests__/security --passWithNoTests",
    "validate:production": "NODE_ENV=production node scripts/validate-production.mjs",
    "setup:encryption": "node scripts/setup-encryption.mjs",
    "gdpr:cleanup": "node scripts/data-retention-cleanup.mjs",
    "gdpr:cleanup:dry": "DRY_RUN=true node scripts/data-retention-cleanup.mjs",
    "perf": "node scripts/performance-monitor.js",
    "perf:watch": "chokidar 'components/**/*' 'lib/**/*' 'app/**/*' -c 'npm run perf'",
    "build:perf": "npm run perf && npm run bundle-report",
    "optimize": "npm run build:perf && npm run size-check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.1.3",
    "@trpc/react-query": "^11.1.3",
    "@trpc/server": "^11.1.3",
    "@types/bcrypt": "^5.0.2",
    "@upstash/redis": "^1.32.0",
    "@vercel/otel": "^1.12.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.15.0",
    "jose": "^6.0.11",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "prisma": "^6.8.2",
    "puppeteer": "^24.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.15.3",
    "resend": "^4.5.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.3",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "vaul": "^1.1.2",
    "web-vitals": "^5.0.1",
    "zod": "^3.25.30",
    "zustand": "^5.0.5"
  },
  "bundlesize": [
    {
      "path": ".next/static/chunks/pages/**/*.js",
      "maxSize": "250kb"
    },
    {
      "path": ".next/static/chunks/framework-*.js",
      "maxSize": "100kb"
    },
    {
      "path": ".next/static/chunks/main-*.js",
      "maxSize": "50kb"
    },
    {
      "path": ".next/static/css/**/*.css",
      "maxSize": "50kb"
    }
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@next/bundle-analyzer": "^15.3.2",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.15.23",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
